#include <stdio.h>

void print_times_table(int n) {
    // Check if n is within the valid range
    if (n < 0 || n > 15) {
        return;
    }

    // Print the n times table
    for (int i = 0; i <= 10; i++) {
        printf("%d x %d = %d\n", i, n, i * n);
    }
}

int main() {
    print_times_table(5);
    return 0;
}

